<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - LogPanel</title>
    
    @await RenderSectionAsync("Styles", required: false)

</head>
<body>
