@using LogPanelViews;
@using LogPanelEntities.Entities;
@model List<Log>

@{
    ViewData["Title"] = "Exceptions Panel";
    Layout = "_LogLayout";
}

@section Styles{
    <link rel="stylesheet" href="~/_content/LogPanelViews/css/exceptions.css" />
    <link rel="stylesheet" href="~/_content/LogPanelViews/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.3/font/bootstrap-icons.css">
}

<partial name="_FullLoggerPartial" model="@Model"/>

@section Scripts{
    <script src="~/_content/LogPanelViews/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/_content/LogPanelViews/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/_content/LogPanelViews/js/exceptions.js" asp-append-version="true"></script>
}